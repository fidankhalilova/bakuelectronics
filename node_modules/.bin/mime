#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/parvin/Desktop/bakuelectronics/node_modules/mime/bin/node_modules:/Users/parvin/Desktop/bakuelectronics/node_modules/mime/node_modules:/Users/parvin/Desktop/bakuelectronics/node_modules:/Users/parvin/Desktop/node_modules:/Users/parvin/node_modules:/Users/node_modules:/node_modules:/Users/parvin/Desktop/bakuelectronics/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/parvin/Desktop/bakuelectronics/node_modules/mime/bin/node_modules:/Users/parvin/Desktop/bakuelectronics/node_modules/mime/node_modules:/Users/parvin/Desktop/bakuelectronics/node_modules:/Users/parvin/Desktop/node_modules:/Users/parvin/node_modules:/Users/node_modules:/node_modules:/Users/parvin/Desktop/bakuelectronics/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/bin/cli.js" "$@"
else
  exec node  "$basedir/../mime/bin/cli.js" "$@"
fi
